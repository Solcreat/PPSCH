from functools import reduce
# ['Женя', 'Вася']
print(list(map(lambda x: x ** 3, [2, 4, 6, 8, 10])))
# [8, 64, 216, 512, 1000]
print(list(filter(lambda x: x < 0, [-1, 4, -7, -8, -10, 1, 0])))
# [-1, -7, -8, -10]
print(reduce(lambda x, y: x * y, [int(x) for x in range(1, int(input()) + 1)]))
# 40320
print(max(list(filter(lambda x: (x**2 % 9) == 0, [2, 4, 6, 8, 0, 3, 4, 2, 3, 5, 1, 2]))))
# 6
